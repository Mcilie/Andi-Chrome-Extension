{
  "manifest_version": 3,
  "name": "Andi Search",
  "version": "1.0",
  "description": "Change the default search engine to Andi Search",
  
  
  "permissions": ["activeTab", "tabs", "storage"],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Andi Search",
      "keyword": "andisearch",
      "search_url": "https://andisearch.com/nav/?query={searchTerms}",
      "favicon_url": "https://andisearch.com/favicon.ico",
      "encoding": "UTF-8",
      "is_default":true
    }
  }
}
